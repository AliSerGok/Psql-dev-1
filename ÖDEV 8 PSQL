3-)
    3.1) UPDATE employee
         SET name = 'Jhon Kidman'
         WHERE id = 17;
    
    3.2) UPDATE employee
         SET email = 'unknown'
         WHERE email LIKE '%github%' OR email LIKE '%example%';

    3.3) UPDATE employee
         SET name = 'Juniour'
         WHERE birthday > '2022-01-01';
         
    3.4) UPDATE employee
         SET email = 'xxxx@xmail.com'
         WHERE name='Juniour';     
         
    3.5) UPDATE employee
         SET id= 10,
             name= 'Hasan',
	           birthday = '1995-02-08',
	           email='hasan@gmail.com'
         WHERE id=17;
         
4-)
     4.1) DELETE FROM employee
          WHERE id = 19;

     4.2) DELETE FROM employee
          WHERE email LIKE '%unknown%';
    
     4.3) DELETE FROM employee
          WHERE name ='Juniour';
     
     4.4) DELETE FROM employee
          WHERE birthday > '2020-01-01';
     
     4.5) DELETE FROM employee
          WHERE name = 'Hasan';






     
         
         
         
         
         
         
         
         
         
         
         
         
         
         
         



